<a href="/"><- Ir al formulario</a>
<h2>Vista de Productos</h2>
{{#if products.length }}
  <table>
    <tr>
      <th>Title</th>
      <th>Price</th>
      <th>Thumbnail</th>
    </tr>
    {{#each products}}
      <tr>
        <td>{{title}}</td>
        <td>{{price}}</td>
        <td><img src="{{thumbnail}}" alt=""></td>
      </tr>
    {{/each}}
  </table>
{{else}}
  <h3>No hay datos.</h3>
{{/if}}